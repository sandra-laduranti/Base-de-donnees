/*BdD ouf_tim */
/*Procedure de creation de location */
/*Auteur(s): PATRICK POL*/
/*Testeur(s):PATRICK POL */
/*Intégrateur: SANDRA LADURANTI*/


/* Test 1 */

USE ouf_tim
GO

Declare @debut DATE
SET @debut = CURRENT_TIMESTAMP+1;
Declare @finp DATE
SET @finp = CURRENT_TIMESTAMP+2;
Declare @finr DATE
SET  @finr = CURRENT_TIMESTAMP+3;

EXEC Locations_creer
    @Date_debut = @debut,
    @Duree = 24,
    @Date_fin_prevue = @finp,
    @Date_fin_reelle = @finr,
    @FK_exemplaire = 1,
     @FK_num_abonnement = 1
    

Select * from Locations



/* Test 2 */


USE ouf_tim
GO

Declare @debut DATE
SET @debut = CURRENT_TIMESTAMP+1;
Declare @finp DATE
SET @finp = CURRENT_TIMESTAMP+40;
Declare @finr DATE
SET  @finr = CURRENT_TIMESTAMP+3;

EXEC Locations_creer
    @Date_debut = @debut,
    @Duree = 600,
    @Date_fin_prevue = @finp,
    @Date_fin_reelle = @finr,
    @FK_exemplaire = '',
     @FK_num_abonnement = 1


Select * from Locations



/* Test 3 */


USE ouf_tim
GO

Declare @debut DATE
SET @debut = CURRENT_TIMESTAMP+1;
Declare @finp DATE
SET @finp = CURRENT_TIMESTAMP+2;
Declare @finr DATE
SET  @finr = CURRENT_TIMESTAMP+3;

EXEC Locations_creer
    @Date_debut = @debut,
    @Duree = 200,
    @Date_fin_prevue = @finp,
    @Date_fin_reelle = @finr,
    @FK_exemplaire = '',
     @FK_num_abonnement = 1

Select * from Locations


/* Test 4 */


USE ouf_tim
GO

Declare @debut DATE
SET @debut = CURRENT_TIMESTAMP+1;
