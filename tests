-- Remplissage en exemplaire pour les editions

USE "ouf_tim"
GO
DECLARE @Dinser DATETIME
SET @Dinser = CURRENT_TIMESTAMP;
DECLARE @Dappro DATETIME
SET @Dappro = CURRENT_TIMESTAMP;
EXEC insere_exemplaire
@numSerieEdition = 7,
@nombreEx = 10,

@NomE = 'Unlimited',
@AnneeE = 2013,
@Duree = 95,
@limiteA =12,
@SupportE ='Bluray',
@Dinsertion =@Dinser,
@Tcouleur = 'Couleur',
@Tson = '13.9',

@Date_approvisionnement = @Dappro,
@Taux_usure = 1,
@Disponibilite = 1,
@Reserve = 0,
@FK_numero_edition = 7

SELECT * FROM Exemplaires

