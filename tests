/*Procedure de creation d'un type d'abonnement					 */
/*Auteur(s): Tsvetkov Alexandre Laduranti Sandra Elise Brochen   */
/*Intégrateur: Laduranti Sandra								     */

/* Test 1 (marche) */
USE ouf_tim

GO
EXEC Type_abonnement_creer
    @Nom = 'Familiale',
    @Prix = 30,
    @Prix_location = 5,
    @Exemplaires_simultanes = 3,
    @Prix_retard = 200,
    @Duree_engagement = 1 /* an */

Select * from Type_abonnement


/* Test 2 (marche pas) */
USE ouf_tim

GO
EXEC Type_abonnement_creer
    @Nom = ' ',     /* impossible */
    @Prix = 30,
    @Prix_location = 5,
    @Exemplaires_simultanes = 3,
    @Prix_retard = 200,
    @Duree_engagement = 0.1 

Select * from Type_abonnement

/* Test 3 (marche pas) */
USE ouf_tim

GO
EXEC Type_abonnement_creer
    @Nom = 'haha',
    @Prix = -50,        /* impossible */
    @Prix_location = 5,
    @Exemplaires_simultanes = 60,
    @Prix_retard = 200,
    @Duree_engagement = 0.1 

Select * from Type_abonnement

/* Test 4 (marche pas) */
USE ouf_tim

GO
EXEC Type_abonnement_creer
    @Nom = '$',     /* impossible */
    @Prix = 50,     
    @Prix_location = 5,
    @Exemplaires_simultanes = 60,
    @Prix_retard = 200,
    @Duree_engagement = 0.1 

Select * from Type_abonnement

/* Test 5 */
USE ouf_tim

GO
EXEC Type_abonnement_creer
    @Nom = 'ceci est un nom composé',
    @Prix = 50,     
    @Prix_location = 5,
    @Exemplaires_simultanes = 30,
    @Prix_retard = 200,
    @Duree_engagement = 10 

Select * from Type_abonnement

