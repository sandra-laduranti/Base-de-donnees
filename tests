/* Test 1 */

USE ouf_tim

GO

EXEC Edition_creer
    @Nom = 'much',
    @Annee_de_sortie = '2012',
    @Duree_film = 120,
    @limite_age = 12,
    @Support = 'BluRay',
    @Date_insertion = '2013',
    @Type_couleur = 'Couleur',
    @Type_son = '5.1',
    @FK_titre = 'Terminator',
    @FK_complement_titre = '1'
    
Select * from Edition


/* Test 2 */

USE ouf_tim

GO
EXEC Edition_creer
    @Nom = '',
    @Annee_de_sortie = '2012',
    @Duree_film = 120,
    @limite_age = 12,
    @Support = 'BluRay',
    @Date_insertion = '2013',
    @Type_couleur = 'Couleur',
    @Type_son = '5.1',
    @FK_titre = 'X-Men',
    @FK_complement_titre = 'Le Commencement'
    

Select * from Edition


