/* Test Abonnement */

USE ouf_tim

GO

Declare @Dcrea DATE
SET @Dcrea = CURRENT_TIMESTAMP+1;
Declare @Dexpi DATE
SET @Dexpi = CURRENT_TIMESTAMP+40;

EXEC Abonnement_creer
    @Date_creation = @Dcrea,
    @Date_expiration = @Dexpi,
    @FK_numcompte = 1,
    @FK_type_abonnement = 'familiale'


Select * from Abonnement



/* Test Abonnement */

USE ouf_tim

GO

Declare @Dcrea DATE
SET @Dcrea = CURRENT_TIMESTAMP+1;
Declare @Dexpi DATE
SET @Dexpi = CURRENT_TIMESTAMP+40;

EXEC Abonnement_creer
    @Date_creation = @Dcrea,
    @Date_expiration = '1900-12-30 22:21:20',
    @FK_numcompte = 1,
    @FK_type_abonnement = 'familiale'


Select * from Abonnement


