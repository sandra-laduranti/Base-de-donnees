-- ANNULATION LIVRAISON
--Annulation reussi
USE "ouf_tim"
GO
DECLARE @Dannulation DATETIME
SET @Dannulation = CURRENT_TIMESTAMP;
EXEC annulation_livraison_creer
	@HD_annulation = @Dannulation,
	@FK_num_reservation = 3,
	@FK_num_abonnement = 2
SELECT * FROM annulation_livraison 

--Annulation impossible
USE "ouf_tim"
GO
DECLARE @Dannulation DATETIME
SET @Dannulation = CURRENT_TIMESTAMP;
EXEC annulation_livraison_creer
	@HD_annulation = @Dannulation,
	@FK_num_reservation = 5,
	@FK_num_abonnement = 2
SELECT * FROM annulation_livraison 


