/*Procedure de creation de relance_retards */
/*Auteur(s): BROCHEN Elise                 */
/*testeur(s): BROCHEN Elise                */
/*Intégrateur: LADURANTI Sandra            */

/* Relance Retard */

GO

Declare @Dcrea DATE
SET @Dcrea = CURRENT_TIMESTAMP+1;
Declare @Dmaj DATE
SET @Dmaj = CURRENT_TIMESTAMP+2;

EXEC Relance_retards_creer
    @Niveau_de_retard = 1,
    @Date_creation = @Dcrea,
    @Date_mise_a_jour = @Dmaj,
    @FK_num_location = 1

Select * from Relance_retards

/* Relance Retard */

GO

Declare @Dcrea DATE
SET @Dcrea = CURRENT_TIMESTAMP+1;
Declare @Dmaj DATE
SET @Dmaj = CURRENT_TIMESTAMP+2;

EXEC Relance_retards_creer
    @Niveau_de_retard = 2,
    @Date_creation = @Dcrea,
    @Date_mise_a_jour = @Dmaj,
    @FK_num_location = 1

select * from Relance_retards


/* Relance Retard */

GO

Declare @Dcrea DATE
SET @Dcrea = CURRENT_TIMESTAMP+1;
Declare @Dmaj DATE
SET @Dmaj = CURRENT_TIMESTAMP+2;

EXEC Relance_retards_creer
    @Niveau_de_retard = 2,
	 @Date_creation = @Dcrea,	
    @Date_mise_a_jour = @Dmaj,
    @FK_num_location = 1

select * from Relance_retards

